{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build-ci": "npm run js-build && eleventy && npm run test",
    "build": "npm run clean && npm run build-ci",
    "watch": "concurrently \"npm run serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "serve": "npm run clean && eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch",
    "clean": "rimraf ./_site/posts/* && rimraf ./_site/short-bits/*"
  },
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.2.2",
    "@piraces/eleventy-plugin-pwa": "^1.1.1",
    "file-type": "^17.1.1",
    "fs-extra": "^10.0.0",
    "jsdom": "^19.0.0",
    "luxon": "^2.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "node-fetch": "^3.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-upgrade-help": "^1.0.1",
    "clean-css": "^5.2.2",
    "concurrently": "^7.0.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^5.0.2",
    "datauri": "^4.1.0",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.2",
    "html-minifier": "^4.0.0",
    "image-size": "^1.0.1",
    "jimp": "^0.16.1",
    "lru-cache": "^6.0.0",
    "mocha": "^9.2.0",
    "phin": "^3.6.1",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.1.3",
    "querystring": "^0.2.1",
    "rollup": "^2.66.1",
    "sharp": "^0.29.3",
    "twitter": "^1.7.1"
  }
}
